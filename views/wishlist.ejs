	<div class="container">

        <h1>Wishlist</h1>
    
        <div class="cart">

            <div class="products">
                <%let sum1=0 ;%>
                <% product.forEach(product=> { %>
                    <%sum1=sum1+product.Price ;%>

                    <div class="product">
                        <a href="/item/<%=product.id%>" class="product-link">
                          <img src="../images/<%=product.image%>" alt="Product Image" class="product-image">
                          <div class="product-info">
                            <h3 class="product-name"><%=product.title%></h3>
                            <p class="product-price">$<%=product.Price%></p>
                            <p class="product-offer"><%=product.category%></p>
                          </div>
                          <a class="product-remove" href="/wishlist/<%=product.id%>"><i class="fa-solid fa-trash"></i></a>

                        <!-- </a> -->
                      </div>

                      <%})%>
