                 <input type="text" name="Type" class="in" placeholder="user Type"  class="textBox" value="<%=data.Type%>" ><br>
                                          
                     




                      <input type="hidden" name="_id" value="<%=data._id%>">
    
                  <div class="submit-btn"><button type="submit">Submit</button></div>
                  
                 
